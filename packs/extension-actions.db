{"_id":"rzKx8VaffGqCtLS5","name":"Rebound","type":"action","img":"systems/pf2e/icons/actions/Passive.webp","effects":[],"folder":null,"sort":0,"flags":{"core":{"sourceId":"Item.noKNh7wc10jdQuDn"}},"system":{"description":{"value":"<p>You spend 10 minutes recuperating. This restores your ability to use exhaust actions. To Rebound, you cannot use actions or activities that are mentally or physically exerting, as determined by the GM. In general, anything that would interrupt an 8-hour rest disrupts the Rebound activity.</p>"},"source":{"value":""},"traits":{"value":["concentrate","exploration"],"rarity":"common","custom":""},"rules":[],"slug":null,"schema":{"version":0.794,"lastMigration":{"datetime":"2022-10-15T22:00:32.905-04:00","version":{"schema":0.73,"foundry":"10.288","system":"4.2.6"}}},"actionType":{"value":"passive"},"actionCategory":{"value":"interaction"},"actions":{"value":null},"requirements":{"value":""},"trigger":{"value":""},"deathNote":false},"ownership":{"default":0,"wIebhbqdYwooA4IF":3},"_stats":{"systemId":"pf2e","systemVersion":"4.2.6","coreVersion":"10.288","createdTime":1665885632949,"modifiedTime":1665885865664,"lastModifiedBy":"37P9RfIwb7hkZygJ"}}
